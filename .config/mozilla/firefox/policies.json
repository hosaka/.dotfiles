{
    "policies": {
        "AppAutoUpdate": false,
        "AppAutoUpdate_comment": "Change to false to disable auto-updates.",
        "DisableFirefoxStudies": true,
        "DisableTelemetry": true,
        "DisableFeedbackCommands": true,
        "DisablePocket": true,
        "DisableSetDesktopBackground": true,
        "DisableDeveloperTools": false,
        "DontCheckDefaultBrowser": true,
        "ManualAppUpdateOnly": true,
        "ManualAppUpdateOnly_comment": "Change to true to disable auto-updates.",
        "NoDefaultBookmarks": true,
        "SearchEngines": {
            "PreventInstalls": false,
            "Remove": [
                "Bing",
                "DuckDuckGo",
                "Google",
                "Perplexity"
            ],
            "Default": "SearXNG",
            "Add": [
                {
                    "Name": "SearXNG",
                    "Description": "SearXNG",
                    "Alias": "@s",
                    "Method": "GET",
                    "URLTemplate": "https://search.hosaka.cc/?q={searchTerms}",
                    "IconURL": "data:image/png;base64,"
                },
                {
                    "Name": "Google (no AI)",
                    "Description": "Google (no AI)",
                    "Alias": "@g",
                    "Method": "GET",
                    "URLTemplate": "https://www.google.com/search?udm=14&q={searchTerms}",
                    "IconURL": "data:image/png;base64,"
                }
            ]
        }
    }
}